var value = 26;
var pace = 1;
var myVar = setInterval(function(){
		value=value + pace ;
		$('#percent-tag').html(value+"%");
$('.progress-bar').css('width', value+'%').attr('aria-valuenow', value);
	if(value < 30){ pace = 1;}else{pace = 3;}
	if(value > 85){ pace = 2;}else{pace = 3;}
	if(value >=99){
		$("#sold_tile").html("<%= escape_javascript(render('admin/modals/vehicle_status')) %>")
		$('.sold_btn' ).hide();
		$('#sold_modal').modal('show').fadeIn(230)
		$("#loading-screen").fadeOut(50); 
	    clearInterval(myVar);

	}
}, 60);
