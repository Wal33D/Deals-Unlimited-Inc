<script>
$('#pics').contents().filter(function(){ return !(this.tagName == 'DIV'
|| this.tagName == 'A');}).remove();
jQuery(document).ready(function($){try{setTimeout("$('#pics-section').slideDown(2000);", 1000);}catch(err){}});  
</script>