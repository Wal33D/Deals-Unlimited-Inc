<% else %>
<section  id="pics-section" style="display:none; overflow-y:hidden!important;">
  <div id="pics" >
    <%= @vehicle.vehicle_images.each do |p| %>
    <a class="vehicle_thumb" href="<%= p.url %>" data-lightbox="<%= @vehicle.title %> Images" data-title="<%= @vehicle.title %>">
      <%=image_tag p.url %>
    </a>
    <% end %>
  </div>
</section>
